{"ast":null,"code":"import { mapState, mapGetters } from 'vuex'; // import Datepicker from 'vuejs-datepicker';\n\nexport default {\n  components: {// Datepicker\n  },\n\n  data() {\n    const times = [];\n\n    for (let i = 1; i <= 24; i++) {\n      times.push(i + ':00');\n    }\n\n    return {\n      times,\n      event: this.createFreshEventObject()\n    };\n  },\n\n  computed: { ...mapGetters(['getEventById']),\n    ...mapState(['categories'])\n  },\n  methods: {\n    createEvent() {\n      this.$store.dispatch('createEvent', this.event).then(() => {\n        this.$router.push({\n          name: 'event-show',\n          params: {\n            id: this.event.id\n          }\n        });\n        this.event = this.createFreshEventObject();\n      }).catch(() => {\n        console.log('There was a problem creating your event');\n      });\n    },\n\n    createFreshEventObject() {\n      const user = user;\n      const id = Math.floor(Math.random() * 10000000);\n      return {\n        id: id,\n        organizer: this.computed.user,\n        categorie: '',\n        title: '',\n        description: '',\n        location: '',\n        date: '',\n        time: '',\n        attendees: []\n      };\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6CA,SAASA,QAAT,EAAkBC,UAAlB,QAAoC,MAApC,C,CACA;;AAEA,eAAe;EACbC,UAAU,EAAC,CACT;EADS,CADE;;EAIbC,IAAI,GAAE;IACJ,MAAMC,KAAI,GAAI,EAAd;;IACA,KAAI,IAAIC,IAAI,CAAZ,EAAeA,KAAK,EAApB,EAAwBA,CAAC,EAAzB,EAA6B;MAC3BD,KAAK,CAACE,IAAN,CAAWD,IAAI,KAAf;IACF;;IACA,OAAO;MACLD,KADK;MAELG,KAAK,EAAE,KAAKC,sBAAL;IAFF,CAAP;EAID,CAbY;;EAcbC,QAAQ,EAAE,EACR,GAAGR,UAAU,CAAC,CAAC,cAAD,CAAD,CADL;IAER,GAAGD,QAAQ,CAAC,CAAC,YAAD,CAAD;EAFH,CAdG;EAkBbU,OAAO,EAAE;IACPC,WAAW,GAAG;MACZ,KAAKC,MAAL,CAAYC,QAAZ,CAAqB,aAArB,EAAoC,KAAKN,KAAzC,EAAgDO,IAAhD,CAAqD,MAAM;QACzD,KAAKC,OAAL,CAAaT,IAAb,CAAkB;UAChBU,IAAI,EAAE,YADU;UAEhBC,MAAM,EAAC;YAACC,EAAE,EAAC,KAAKX,KAAL,CAAWW;UAAf;QAFS,CAAlB;QAIA,KAAKX,KAAL,GAAa,KAAKC,sBAAL,EAAb;MACD,CAND,EAMGW,KANH,CAMS,MAAM;QACbC,OAAO,CA
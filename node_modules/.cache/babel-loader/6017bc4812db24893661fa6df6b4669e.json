{"ast":null,"code":"import { mapState, mapActions } from 'vuex';\nimport NProgress from 'nprogress';\nimport store from '@/store';\nexport default {\n  props: ['id'],\n\n  beforeRouteEnter(routeTo, routeFrom, next) {\n    NProgress.start();\n    store.dispatch('event/fetchEvent', routeTo.params.id).then(() => {\n      NProgress.done();\n    });\n  },\n\n  created() {\n    this.fetchEvent(this.id);\n  },\n\n  computed: { ...mapState({\n      event: state => state.event.event\n    })\n  },\n  methods: mapActions('event', ['fetchEvent'])\n};","map":{"version":3,"mappings":"AA4BA,SAASA,QAAT,EAAkBC,UAAlB,QAAoC,MAApC;AACA,OAAOC,SAAP,MAAsB,WAAtB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEA,eAAe;EACbC,KAAK,EAAE,CAAC,IAAD,CADM;;EAEbC,gBAAgB,CAACC,OAAD,EAAUC,SAAV,EAAqBC,IAArB,EAA0B;IACxCN,SAAS,CAACO,KAAV;IACAN,KAAK,CAACO,QAAN,CAAe,kBAAf,EAAmCJ,OAAO,CAACK,MAAR,CAAeC,EAAlD,EAAsDC,IAAtD,CAA2D,MAAM;MAC/DX,SAAS,CAACY,IAAV;IACD,CAFD;EAGD,CAPY;;EASbC,OAAO,GAAG;IACR,KAAKC,UAAL,CAAgB,KAAKJ,EAArB;EACD,CAXY;;EAadK,QAAQ,EAAC,EACR,GAAGjB,QAAQ,CAAC;MACVkB,KAAK,EAAEC,KAAI,IAAKA,KAAK,CAACD,KAAN,CAAYA;IADlB,CAAD;EADH,CAbK;EAiBbE,OAAO,EAAEnB,UAAU,CAAC,OAAD,EAAS,CAAC,YAAD,CAAT;AAjBN,CAAf","names":["mapState","mapActions","NProgress","store","props","beforeRouteEnter","routeTo","routeFrom","next","start","dispatch","params","id","then","done","created","fetchEvent","computed","event","state","methods"],"sourceRoot":"","sources":["/Users/asmitnepali/Documents/projects/personal/real-world-vue/src/views/EventShow.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"event-header\">\n      <span class=\"eyebrow\">@{{ event.time }} on {{ event.date }}</span>\n      <h1 class=\"title\">{{ event.title }}</h1>\n      <h5>Organized by {{ event.organizer ? event.organizer.name : '' }}</h5>\n      <h5>Category: {{ event.category }}</h5>\n    </div>\n\n    <BaseIcon name=\"map\"><h2>Location</h2></BaseIcon>\n\n    <address>{{ event.location }}</address>\n\n    <h2>Event details</h2>\n    <p>{{ event.description }}</p>\n\n    <h2>Attendees\n      <span class=\"badge -fill-gradient\">{{ event.attendees ? event.attendees.length : 0 }}</span>\n    </h2>\n    <ul class=\"list-group\">\n      <li v-for=\"(attendee, index) in event.attendees\" :key=\"index\" class=\"list-item\">\n        <b>{{ attendee.name }}</b>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport { mapState,mapActions } from 'vuex';\nimport NProgress from 'nprogress'\nimport store from '@/store';\n\nexport default {\n  props: ['id'],\n  beforeRouteEnter(routeTo, routeFrom, next){\n    NProgress.start()\n    store.dispatch('event/fetchEvent', routeTo.params.id).then(() => {\n      NProgress.done()\n    })\n  },\n\n  created() {\n    this.fetchEvent(this.id)\n  },\n\n computed:{\n  ...mapState({\n    event: state => state.event.event\n  })},\n  methods: mapActions('event',['fetchEvent'])\n}\n</script>\n<style scoped>\n.location {\n  margin-bottom: 0;\n}\n.location > .icon {\n  margin-left: 10px;\n}\n.event-header > .title {\n  margin: 0;\n}\n.list-group {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n}\n.list-group > .list-item {\n  padding: 1em 0;\n  border-bottom: solid 1px #e5e5e5;\n}\n</style>"]},"metadata":{},"sourceType":"module"}